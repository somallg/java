<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <servlet>
        <servlet-name>Test</servlet-name>
        <servlet-class>com.TestServlet</servlet-class>
        <init-param>
            <param-name>ice</param-name>
            <param-value>tea</param-value>
        </init-param>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/Test/Test</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Target</servlet-name>
        <servlet-class>com.TargetServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Target</servlet-name>
        <url-pattern>/target</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Processor</servlet-name>
        <servlet-class>com.ProcessorServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Processor</servlet-name>
        <url-pattern>/abc</url-pattern>
    </servlet-mapping>

    <context-param>
        <param-name>ice</param-name>
        <param-value>tea</param-value>
    </context-param>

    <listener>
        <listener-class>com.MyListener</listener-class>
    </listener>
    
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <session-config>
        <session-timeout>10</session-timeout>
    </session-config>

